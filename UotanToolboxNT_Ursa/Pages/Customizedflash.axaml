<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             xmlns:assets="clr-namespace:UotanToolboxNT_Ursa.Assets"
			 xmlns:vm="using:UotanToolboxNT_Ursa.ViewModels"
             xmlns:collections="clr-namespace:Avalonia.Collections;assembly=Avalonia.Base"
			 xmlns:controls="clr-namespace:UotanToolboxNT_Ursa.Controls"
             xmlns:iri="https://irihi.tech/shared"
			 xmlns:u="https://irihi.tech/ursa"
             mc:Ignorable="d" d:DesignWidth="980" d:DesignHeight="830"
             Background="{DynamicResource PageBackground}"
             x:Class="UotanToolboxNT_Ursa.Pages.Customizedflash"
			 x:DataType="vm:CustomizedflashViewModel">

     <ScrollViewer VerticalScrollBarVisibility="Hidden">
            <StackPanel>
				<Border Width="960"
						Height="240"
						Margin="10"
						CornerRadius="15"
						Theme="{StaticResource CardBorder}">
					<StackPanel Spacing="30">
						<StackPanel>
							<TextBlock HorizontalAlignment="Left"
									   VerticalAlignment="Top"
									   FontWeight="DemiBold"
									   Text="{DynamicResource Customizedflash_PresetPartitionFlash}" />
							<StackPanel Orientation="Horizontal" Spacing="20">
								<StackPanel Spacing="10">
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="50"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="System" />
										<TextBox Width="270"
												 Classes="Bordered"
												/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="50"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="Product" />
										<TextBox Width="270"
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="50"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="Vendor" />
										<TextBox Width="270" 
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="50"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="Boot" />
										<TextBox Width="270" 
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
								</StackPanel>
								<StackPanel Spacing="10">
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="80"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="System_ext" />
										<TextBox Width="240" 
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="80"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="Odm" />
										<TextBox Width="240"
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="80"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="Vender_boot" />
										<TextBox Width="240"
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
									<StackPanel Margin="0" Orientation="Horizontal">
										<TextBlock Width="80"
												   Margin="8,0,0,0"
												   VerticalAlignment="Center"
												   FontSize="13"
												   Text="Init_boot" />
										<TextBox Width="240"
												 Classes="Bordered"
												 />
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_File}"/>
										<Button Theme="{DynamicResource OutlineButton}"
												Height="35"
												Margin="5,0,0,0"
												Padding="10,0,10,0"
												
												Content="{DynamicResource Customizedflash_Flash}"/>
									</StackPanel>
								</StackPanel>
							</StackPanel>
						</StackPanel>
					</StackPanel>
				</Border>

				<StackPanel Orientation="Horizontal">
					<Border Width="750"
						Height="120"
						Margin="10"
						CornerRadius="15"
						Theme="{StaticResource CardBorder}">
						<StackPanel>
							<TextBlock HorizontalAlignment="Left"
												   VerticalAlignment="Top"
												   FontWeight="DemiBold"
												   Text="{DynamicResource Customizedflash_CustomizedPartFlash}" />
							<StackPanel Margin="0,10,0,0" Orientation="Horizontal" Spacing="5">
								<Button Theme="{DynamicResource OutlineButton}"
										Height="35"
										Padding="10,0,10,0"
										
										Content="{DynamicResource Customizedflash_File}"/>
								<TextBox Width="350"
										 Classes="Bordered"
										 />
								<TextBlock Width="55"
										   FontSize="15"
										   VerticalAlignment="Center"
										   Text="{DynamicResource Customizedflash_FlashTo}" />
								<TextBox Width="160"
										 Classes="Bordered"
										 />
								<Button Theme="{DynamicResource OutlineButton}"
										Height="35"
										Margin="5,0,0,0"
										Padding="10,0,10,0"
										
										Content="{DynamicResource Customizedflash_Flash}"/>
							</StackPanel>
						</StackPanel>
					</Border>

					<Border Width="190"
						Height="120"
						Margin="10"
						CornerRadius="15"
						Theme="{StaticResource CardBorder}">
						<StackPanel>
							<StackPanel Orientation="Vertical" Spacing="10">
								<Button Theme="{DynamicResource OutlineButton}"
										Height="35" Width="150"
										
										Content="{DynamicResource Customizedflash_DisableVbmeta}" />
								<Button Theme="{DynamicResource OutlineButton}"
										Height="35" Width="150"
										
										Content="{DynamicResource Customizedflash_SwitchSlot}" />
							</StackPanel>
						</StackPanel>
					</Border>
				</StackPanel>

				<Border Width="960"
						Height="415"
						Margin="10"
						CornerRadius="15"
						Theme="{StaticResource CardBorder}">
					<TextBox Margin="0 0 0 0"
							 CornerRadius="15"
							 Width="930" Height="385"
							 Watermark="{DynamicResource Common_Log}"
							 Classes="TextArea"
							 Text=""
							 TextWrapping="Wrap"
							 IsReadOnly="True"/>
				</Border>
            </StackPanel>
        </ScrollViewer>
</UserControl>
